Funcion sencostan (a)
	
	Escribir "El seno del angulo es: ", sen(a);
	Escribir "El coseno del angulo es: ", cos(a);
	Escribir "La tangente del angulo es: ", tan(a);
	
FinFuncion

Algoritmo Fifty_program
	
	Definir ang Como Real;
	
	Escribir "Defina el angulo (en radianes): ";
	Leer ang;
	
	sencostan(ang);
	
FinAlgoritmo
