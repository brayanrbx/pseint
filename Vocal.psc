Algoritmo Twentyt_program
	
	Definir num Como Entero;
	
	Escribir "Ingrese un numero del 1 al 5";
	Leer num;
	Segun num Hacer
		1:	
			Escribir "a";
		2:	
			Escribir "e";
		3: 	
			Escribir "i";
		4:
			Escribir "o";
		5:	
			Escribir "u";
		De Otro Modo:
			Escribir "Numero invalido";
	FinSegun
	
	
FinAlgoritmo
