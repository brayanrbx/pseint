Algoritmo Six_program
	
	Definir superfi Como Real;
	Definir longi Como Real;
	Definir volumen Como Real;
	
	Escribir "Longitud de una de las caras del cubo: ";
	Leer longi;
	superfi <- (longi ^ 2) * 6;
	volumen <- longi ^ 3;
	
	Escribir "La superficie del cubo es: ", superfi;
	Escribir "El volumen del cubo es: ", volumen;
	
FinAlgoritmo
