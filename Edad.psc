Algoritmo Nineteen_program
	
	Definir name Como caracter;
	Definir yearb Como Entero;
	
	Escribir "Escriba su nombre: ";
	Leer name;
	Escribir "Escriba su año de nacimiento: ";
	leer yearb;
	
	Si (yearb <= 2002) Entonces
		Escribir name, " Es mayor de edad";
	SiNo
		Escribir name, " No es mayor de edad";
	FinSi
								
FinAlgoritmo
